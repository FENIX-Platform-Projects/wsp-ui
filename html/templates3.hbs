<script type='text/x-handlebars-template' id='wsp_ui_structure'>
    <div class='container'>
        <div class='row'>
            <div class='col-xs-12'>
                <h1>
                    <img src='images/fx-logo.png' style='width: 60px; heigh: 60px;'>
                    {{wsp}}
                </h1>
            </div>
        </div>
        <div id="wsp-landing">
            <div class='row'>
                <div class='col-xs-2'>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="wheat">
                        {{{wheat}}}
                    </button>
                </div>
                <div class='col-xs-2'>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="population_landscan">
                        {{{population_landscan}}}
                    </button>
                </div>
                <button class='btn btn-primary btn-xs wsp-btn' data-role="rainfed_land_gaez">
                    {{{rainfed_land_gaez}}}
                </button>
                <div class='col-xs-2'>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="irrigated_areas_solaw_2012">
                        {{{irrigated_areas_solaw_2012}}}
                    </button>
                </div>
                <div class='col-xs-2'>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="cultivated_land_gaez_2010">
                        {{{cultivated_land_gaez_2010}}}
                    </button>
                </div>
            </div>
            <div class='row'>
                <div class='col-xs-12'>
                    <div data-role="map" style="height: 650px; overflow: hidden;"></div>
                </div>
            </div>
        </div>
        <div id="wsp-tool">
            <div class='row'>
                <div class='col-xs-2'>
                    <div class='col-xs-12'>
                        <h4>{{{selectable_layers}}}</h4>
                    </div>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="wheat">
                        {{{wheat}}}
                    </button>
                    <!--<label><input type="checkbox" data-toggle="toggle" data-role="wheat">{{{wheat}}}</label>-->

                    <button class='btn btn-primary btn-xs wsp-btn' data-role="population_landscan">
                        {{{population_landscan}}}
                    </button>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="rainfed_land_gaez">
                        {{{rainfed_land_gaez}}}
                    </button>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="irrigated_areas_solaw_2012">
                        {{{irrigated_areas_solaw_2012}}}
                    </button>
                    <button class='btn btn-primary btn-xs wsp-btn' data-role="cultivated_land_gaez_2010">
                        {{{cultivated_land_gaez_2010}}}
                    </button>
                </div>
                <div class='row'>
                    <div class='col-xs-12'>
                        {{#each box}}
                            <div class='col-xs-6'>
                                <div class='row box' id='{{box_id}}'>
                                    <div data-role='box-header'>
                                        <div class='row'>
                                            <div class='col-xs-12'>
                                                <h4>{{{box_title}}}</h4>
                                            </div>
                                        </div>
                                        <div class='row'>
                                            <div class='col-xs-6'>
                                                <select style='width: 100%;' data-role="dd" class='select2'>
                                                </select>
                                            </div>
                                            <div class='col-xs-3'>
                                                <button class='btn btn-primary btn-xs wsp-btn' data-role="anomaly">
                                                    {{{anomaly}}}
                                                </button>
                                            </div>
                                            {{#if this.zscore}}
                                                <div class='col-xs-3'>
                                                    <button class='btn btn-primary btn-xs wsp-btn' data-role="zscore">
                                                        {{{z_score}}}
                                                    </button>
                                                </div>
                                            {{/if}}
                                        </div>
                                    </div>
                                    <hr>
                                    <div data-role='box-content' data-role="chart-content">
                                        <div class='row'>
                                            <div class='col-xs-12'>
                                                <div data-role="map" style="height: 450px; overflow: hidden;"></div>
                                            </div>
                                            <div class='col-xs-12'>
                                                <div data-role="chart"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div data-role='box-footer'>
                                        <div class='pull-right'>
                                            {{{footer_text}}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                            {{#if this.add_new_line}}
                                <!--</div>-->
                                <hr>
                                <!--<div class='row'>-->
                            {{/if}}
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>
</script>